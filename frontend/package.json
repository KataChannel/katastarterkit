{
  "name": "timonacore-frontend",
  "version": "1.1.0",
  "description": "Timonacore Frontend - Next.js with TailwindCSS and GraphQL",
  "private": true,
  "scripts": {
    "dev": "next dev -p 13000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "bun test",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:e2e:ci": "bun run build && bun run start & cypress run --headless --browser chrome",
    "test:e2e:dev": "bun run dev & cypress open",
    "test:all": "bun run type-check && bun run lint && bun run test && bun run test:e2e:headless",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@apollo/client": "3.11.8",
    "@headlessui/react": "^2.1.8",
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@tailwindcss/forms": "^0.5.10",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "graphql": "^16.9.0",
    "graphql-ws": "^6.0.6",
    "next": "14.2.5",
    "next-auth": "^4.24.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "yup": "^1.4.0",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.6",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-onboarding": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/nextjs": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^15.0.0",
    "eslint": "^9.11.1",
    "eslint-config-next": "15.0.0-rc.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.47",
    "storybook": "^8.3.5",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2"
  }
}
