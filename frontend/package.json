{
  "name": "rausachcore-frontend",
  "version": "1.0.0",
  "description": "Modern Frontend with Next.js 15, React 19, TailwindCSS v4, and GraphQL",
  "private": true,
  "scripts": {
    "dev": "next dev -p 12000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headless": "playwright test --project=chromium",
    "test:e2e:ci": "bun run build && bun run start & cypress run --headless --browser chrome",
    "test:e2e:dev": "bun run dev & cypress open",
    "test:all": "bun run type-check && bun run lint && bun run test && bun run test:e2e:headless",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "analyze": "ANALYZE=true next build",
    "bundle:analyze": "bash ./analyze-bundle.sh"
