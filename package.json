{
  "name": "katacore-starter",
  "version": "1.0.0",
  "description": "Modern Fullstack Starter Kit with Next.js + NestJS + GraphQL + Prisma + Redis + TailwindCSS v4",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"bun run dev:backend\" \"bun run dev:frontend\"",
    "dev:backend": "cd backend && bun run dev",
    "dev:frontend": "cd frontend && bun run dev",
    "build": "bun run build:backend && bun run build:frontend",
    "build:backend": "cd backend && bun run build",
    "build:frontend": "cd frontend && bun run build",
    "test": "bun run test:backend && bun run test:frontend",
    "test:backend": "cd backend && bun test",
    "test:frontend": "cd frontend && bun test",
    "lint": "bun run lint:backend && bun run lint:frontend",
    "lint:backend": "cd backend && bun run lint",
    "lint:frontend": "cd frontend && bun run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "docker:devfull": "docker compose up -d",
    "docker:dev": "docker compose -f 'docker-compose.yml' up -d --build 'postgres' 'redis' 'minio'",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker compose down",
    "setup": "bun install && cd backend && bun install && cd ../frontend && bun install",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules bun.lockb backend/bun.lockb frontend/bun.lockb",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:reset": "cd backend && npx prisma migrate reset",
    "db:seed": "cd backend && npx prisma db seed",
    "db:studio": "cd backend && npx prisma studio",
    "db:pushforce": "cd backend && npx prisma db push --force-reset",
    "db:push": "cd backend && npx prisma db push",
    "db:restore": "cd backend && bun run prisma/restore.ts",
    "db:backup": "cd backend && bun run prisma/backup.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2"
  },
  "engines": {
    "bun": ">=1.1.0"
  },
  "keywords": [
    "bun",
    "bunjs",
    "fullstack",
    "typescript",
    "starter-kit",
    "nextjs",
    "nestjs",
    "graphql",
    "prisma",
    "postgresql",
    "redis",
    "minio",
    "docker",
    "tailwindcss",
    "apollo",
    "enterprise",
    "modern",
    "fast"
  ],
  "author": "Katacore Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KataChannel/katastarterkit.git"
  },
  "homepage": "https://github.com/KataChannel/katastarterkit#readme",
  "bugs": {
    "url": "https://github.com/KataChannel/katastarterkit/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "graphql-request": "^7.2.0"
  }
}
