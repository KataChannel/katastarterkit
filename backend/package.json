{
  "name": "rausachcore-backend",
  "version": "1.0.0",
  "description": "Modern Backend with NestJS, GraphQL, Prisma, Redis, and Authentication",
  "author": "rausachcore Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "dev:bun": "bun --hot src/main.ts",
    "dev:stable": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:debug": "bun --inspect src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "check-env": "bun check-env.ts",
    "db:migrate": "bun prisma migrate dev",
    "db:migrate:prod": "bun prisma migrate deploy",
    "db:studio": "bun prisma studio",
    "db:seed": "bun prisma db seed",
    "db:reset": "bun prisma migrate reset",
    "db:generate": "bun prisma generate",
    "seed:demo": "ts-node scripts/seed-demo-pages.ts",
    "cleanup:demo": "ts-node scripts/cleanup-demo-pages.ts",
    "reseed:demo": "npm run cleanup:demo && npm run seed:demo",
    "seed:rbac": "ts-node prisma/seeds/rbac-roles-permissions.seed.ts",
    "seed:assign-roles": "ts-node prisma/seeds/assign-user-roles.seed.ts",
    "seed:source-documents": "ts-node prisma/seeds/seed-source-documents.ts",
    "check:source-documents": "ts-node prisma/seeds/check-source-documents.ts",
    "migrate:assign-roles": "bun run scripts/assign-default-roles.ts",
    "cleanup:users-seed-perms": "ts-node scripts/cleanup-users-and-seed-permissions.ts",
    "assign:content-manager": "ts-node scripts/assign-content-manager-role.ts",
    "assign:role": "ts-node scripts/assign-role-to-user.ts",
    "user:roles": "ts-node scripts/view-user-roles.ts",
    "fix:permissions": "ts-node scripts/fix-null-permission-names.ts",
    "db:backup": "bun run prisma/backup.ts",
    "db:restore": "bun run prisma/restore.ts",
    "db:restore-optimized": "bun run prisma/restore-optimized.ts",
    "db:restore-benchmark": "bash prisma/benchmark-restore.sh",
    "prisma:seed": "bun run prisma/seed.ts",
    "seed:comprehensive": "bun run src/scripts/run-comprehensive-seeder.ts",
    "orama:reindex": "bun scripts/orama-reindex.ts",
    "migrate:old-data": "bun run migrate-old-data.ts",
    "verify:migration": "bun run verify-migration.ts",
    "import:legacy": "bun run prisma/import-legacy-data.ts",
    "import:blog:rausachtrangia": "bun run import-blog-rausachtrangia.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "^11.1.9",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.9",
    "@orama/orama": "^3.1.16",
    "@prisma/client": "^6.19.0",
    "@types/bcrypt": "^6.0.0",
    "@types/graphql-upload": "^17.0.0",
    "@types/ua-parser-js": "^0.7.39",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cache-manager": "^7.2.5",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dataloader": "^2.2.3",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "graphql": "^16.12.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^1.1.0",
    "graphql-scalars": "^1.25.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^17.0.0",
    "graphql-upload-ts": "^2.1.3",
    "graphql-ws": "^6.0.6",
    "ioredis": "^5.8.2",
    "joi": "^18.0.2",
    "minio": "^8.0.6",
    "multer": "^2.0.2",
    "openai": "^6.9.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "0.34.4",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "ua-parser-js": "^2.0.6",
    "uuid": "^11.1.0",
    "web-push": "^3.6.7",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@nestjs/cli": "^11.0.13",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.7.1",
    "prisma": "^6.19.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
