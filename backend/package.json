{
  "name": "katacore-backend",
  "version": "1.0.0",
  "description": "Modern Backend with NestJS, GraphQL, Prisma, Redis, and Authentication",
  "author": "KataCore Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "dev:bun": "bun --hot src/main.ts",
    "dev:stable": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:debug": "bun --inspect src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:cov": "bun test --coverage",
    "test:debug": "bun test --inspect",
    "test:e2e": "bun test e2e",
    "check-env": "bun check-env.ts",
    "db:migrate": "bun prisma migrate dev",
    "db:migrate:prod": "bun prisma migrate deploy",
    "db:studio": "bun prisma studio",
    "db:seed": "bun prisma db seed",
    "db:reset": "bun prisma migrate reset",
    "db:generate": "bun prisma generate",
    "seed:demo": "ts-node scripts/seed-demo-pages.ts",
    "cleanup:demo": "ts-node scripts/cleanup-demo-pages.ts",
    "reseed:demo": "npm run cleanup:demo && npm run seed:demo",
    "db:backup": "bun run prisma/backup.ts",
    "db:restore": "bun run prisma/restore.ts",
    "prisma:seed": "bun run prisma/seed.ts",
    "seed:comprehensive": "bun run src/scripts/run-comprehensive-seeder.ts",
    "orama:reindex": "bun scripts/orama-reindex.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@elastic/elasticsearch": "^8.15.0",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.6",
    "@orama/orama": "^3.1.14",
    "@types/graphql-upload": "^17.0.0",
    "@types/ua-parser-js": "^0.7.39",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dataloader": "^2.2.3",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "graphql": "^16.11.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^1.1.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^17.0.0",
    "graphql-ws": "^6.0.6",
    "ioredis": "^5.7.0",
    "joi": "^18.0.1",
    "minio": "^8.0.6",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "ua-parser-js": "^2.0.5",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@prisma/client": "^6.14.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
